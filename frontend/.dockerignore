# Ignore node_modules (already installed in the builder stage)
node_modules/

# Ignore Next.js build output (will be copied explicitly)
.next/

# Ignore environment files (unless you need them in the build context)
.env
.env.local
.env.development
.env.production

# Ignore version control and editor files
.git/
.gitignore
.vscode/
.idea/

# Ignore other unnecessary files
README.md
LICENSE
*.log

# Optional: Ignore other OS or tooling files
.DS_Store
*.swp
